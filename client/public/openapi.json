{
  "openapi": "3.0.0",
  "info": {
    "title": "API Documentation for Project: My Project",
    "version": "1.0.0",
    "description": "API documentation for project My Project."
  },
  "paths": {
    "/execute/my-project/summarize-text": {
      "post": {
        "summary": "Execute AI function 'Summarize Texts'",
        "description": "Summarizes english texts to a given number_of_sentences.",
        "operationId": "summarize-text",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "text": {
                    "type": "string"
                  },
                  "number_of_sentences": {
                    "type": "string"
                  }
                },
                "required": ["text", "number_of_sentences"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful execution",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        },
        "security": [
          {
            "ApiKeyAuth": []
          }
        ]
      }
    }
  },
  "components": {
    "securitySchemes": {
      "ApiKeyAuth": {
        "type": "apiKey",
        "in": "header",
        "name": "x-api-key"
      }
    }
  },
  "security": [
    {
      "ApiKeyAuth": []
    }
  ]
}
